<style>
  nav.navbar.navbar-default {
    box-shadow: 0px 2px 3px rgb(10 10 10 / 30%);
  }

  .navbar-default {
    background-color: #27205a;
  }
  .navbar {
    position: relative;
    min-height: 50px;
  }
  @media (min-width: 768px) {
    nav.navbar.navbar-default {
      border-top: 1px solid #0f0c22;
      margin-top: 0;
    }
  }
  .navbar-header:before,
  .navbar-header:after {
    content: " ";
    display: table;
  }

  *:before,
  *:after {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
  }
  .navbar-header:after {
    clear: both;
  }
  .navbar-header:before,
  .navbar-header:after {
    content: " ";
    display: table;
  }
  *:before,
  *:after {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
  }

