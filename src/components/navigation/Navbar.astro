  @media (min-width: 768px) {
    .container-fluid {
      padding-left: 20px;
      padding-right: 20px;
    }
  }
  .container-fluid {
    width: 100%;
    max-width: 1090px;
    padding-left: 4%;
    padding-right: 4%;
    margin: 0px auto;
    overflow: hidden;
  }
  @media (min-width: 768px) {
    .navbar-header {
      float: left;
    }
  }

  @media (min-width: 768px) {
    .navbar-nav {
      float: left;
      margin: 0 0 10px 0;
    }
  }
  .navbar-nav {
    margin: 7.5px 0 0;
  }
  .nav {
    margin-bottom: 0;
    padding-left: 0;
    list-style: none;
  }
  @media (min-width: 768px) {
    .navbar-nav > li {
      float: left;
    }
  }

  .nav > li {
    position: relative;
    display: block;
    color: #cbcace;
  }

  @media (min-width: 768px) {
    nav.navbar.navbar-default .nav > li:first-child > a {
      padding-left: 0;
    }
  }

  @media (min-width: 768px) {
    nav.navbar.navbar-default li > a {
      font-size: 0.85em;
    }
  }
  nav.navbar.navbar-default li > a {
    font-size: 1rem;
    line-height: 1.5;
    font-weight: 400;
    font-style: normal;
    transition: all 0.3s;
  }
  @media (min-width: 768px) {
    .navbar-nav > li > a {
      padding-top: 15px;
      padding-bottom: 15px;
      padding-top: 10px;
      padding-bottom: 10px;
    }
  }
  .navbar-nav > li > a {
    padding-top: 10px;
    padding-bottom: 10px;
    line-height: 20px;
  }
  .nav > li > a {
    position: relative;
    display: block;
    color: #cbcace;
    padding: 10px 25px;
  }
</style>
